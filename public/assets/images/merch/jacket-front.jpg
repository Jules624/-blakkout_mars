<svg width="400" height="400" viewBox="0 0 400 400" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="400" fill="#0a0a0a"/>
  
  <!-- Jacket silhouette -->
  <path d="M80 120 L80 100 Q80 80 100 80 L140 80 Q160 60 200 60 Q240 60 260 80 L300 80 Q320 80 320 100 L320 120 L340 140 L340 200 L320 200 L320 360 Q320 370 310 370 L90 370 Q80 370 80 360 L80 200 L60 200 L60 140 L80 120 Z" fill="#1a1a1a" stroke="#00ff00" stroke-width="2"/>
  
  <!-- Collar -->
  <path d="M140 80 Q200 70 260 80 Q270 75 270 85 Q200 75 130 85 Q130 75 140 80" fill="#1a1a1a" stroke="#00ff00" stroke-width="2"/>
  
  <!-- Zipper -->
  <line x1="200" y1="85" x2="200" y2="350" stroke="#00ff00" stroke-width="2"/>
  <rect x="198" y="100" width="4" height="8" fill="#00ff00"/>
  <rect x="198" y="120" width="4" height="8" fill="#00ff00"/>
  <rect x="198" y="140" width="4" height="8" fill="#00ff00"/>
  <rect x="198" y="160" width="4" height="8" fill="#00ff00"/>
  <rect x="198" y="180" width="4" height="8" fill="#00ff00"/>
  
  <!-- Pockets -->
  <rect x="120" y="220" width="60" height="40" rx="5" fill="none" stroke="#00ff00" stroke-width="1"/>
  <rect x="220" y="220" width="60" height="40" rx="5" fill="none" stroke="#00ff00" stroke-width="1"/>
  
  <!-- Tech Limited patch -->
  <g>
    <rect x="160" y="140" width="80" height="30" rx="3" fill="#00ff00" opacity="0.1" stroke="#00ff00" stroke-width="1"/>
    <text x="200" y="155" text-anchor="middle" fill="#00ff00" font-family="monospace" font-size="12" font-weight="bold">TECH</text>
    <text x="200" y="167" text-anchor="middle" fill="#00ff00" font-family="monospace" font-size="8">LIMITED</text>
  </g>
  
  <!-- Circuit pattern on sleeves -->
  <g opacity="0.6">
    <!-- Left sleeve -->
    <line x1="100" y1="150" x2="100" y2="300" stroke="#00ff00" stroke-width="1"/>
    <line x1="90" y1="180" x2="110" y2="180" stroke="#00ff00" stroke-width="1"/>
    <line x1="90" y1="220" x2="110" y2="220" stroke="#00ff00" stroke-width="1"/>
    <line x1="90" y1="260" x2="110" y2="260" stroke="#00ff00" stroke-width="1"/>
    
    <!-- Right sleeve -->
    <line x1="300" y1="150" x2="300" y2="300" stroke="#00ff00" stroke-width="1"/>
    <line x1="290" y1="180" x2="310" y2="180" stroke="#00ff00" stroke-width="1"/>
    <line x1="290" y1="220" x2="310" y2="220" stroke="#00ff00" stroke-width="1"/>
    <line x1="290" y1="260" x2="310" y2="260" stroke="#00ff00" stroke-width="1"/>
  </g>
  
  <!-- @BLAKKOUT_MARS logo on back collar area -->
  <text x="200" y="320" text-anchor="middle" fill="#00ff00" font-family="monospace" font-size="10" font-weight="bold">@BLAKKOUT_MARS</text>
  
  <!-- Product label -->
  <text x="200" y="390" text-anchor="middle" fill="#00ff00" font-family="monospace" font-size="10" opacity="0.7">TECH LIMITED JACKET</text>
</svg>