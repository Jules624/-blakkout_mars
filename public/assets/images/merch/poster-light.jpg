  <!-- Grid pattern overlay -->
  <g opacity="0.2">
    <defs>
      <pattern id="grid" width="20" height="20" patternUnits="userSpaceOnUse">
        <path d="M 20 0 L 0 0 0 20" fill="none" stroke="#00ff00" stroke-width="0.5"/>
      </pattern>
    </defs>
    <rect x="70" y="70" width="260" height="260" fill="url(#grid)"/>
  </g>
  
  <!-- @BLAKKOUT_MARS signature -->
  <text x="200" y="310" text-anchor="middle" fill="#00ff00" font-family="monospace" font-size="10" opacity="0.6">@BLAKKOUT_MARS</text>
  
  <!-- Product label -->
  <text x="200" y="370" text-anchor="middle" fill="#00ff00" font-family="monospace" font-size="10" opacity="0.7">BLACKOUT EDITION POSTER</text>
  <text x="200" y="390" text-anchor="middle" fill="#00ff00" font-family="monospace" font-size="8" opacity="0.5">50x70cm â€¢ PHOSPHORESCENT INK</text>
