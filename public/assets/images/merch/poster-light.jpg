<svg width="400" height="400" viewBox="0 0 400 400" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="400" fill="#0a0a0a"/>
  
  <!-- Poster frame -->
  <rect x="50" y="50" width="300" height="300" fill="#1a1a1a" stroke="#00ff00" stroke-width="3"/>
  
  <!-- Inner content area -->
  <rect x="70" y="70" width="260" height="260" fill="#000000"/>
  
  <!-- Main design - Blackout theme -->
  <g>
    <!-- Central logo/symbol -->
    <circle cx="200" cy="150" r="40" fill="none" stroke="#00ff00" stroke-width="2"/>
    <circle cx="200" cy="150" r="30" fill="none" stroke="#00ff00" stroke-width="1" opacity="0.6"/>
    <circle cx="200" cy="150" r="20" fill="none" stroke="#00ff00" stroke-width="1" opacity="0.4"/>
    
    <!-- Radiating lines -->
    <line x1="200" y1="110" x2="200" y2="90" stroke="#00ff00" stroke-width="2"/>
    <line x1="200" y1="190" x2="200" y2="210" stroke="#00ff00" stroke-width="2"/>
    <line x1="160" y1="150" x2="140" y2="150" stroke="#00ff00" stroke-width="2"/>
    <line x1="240" y1="150" x2="260" y2="150" stroke="#00ff00" stroke-width="2"/>
    
    <!-- Diagonal lines -->
    <line x1="171" y1="121" x2="157" y2="107" stroke="#00ff00" stroke-width="1"/>
    <line x1="229" y1="121" x2="243" y2="107" stroke="#00ff00" stroke-width="1"/>
    <line x1="171" y1="179" x2="157" y2="193" stroke="#00ff00" stroke-width="1"/>
